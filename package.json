{
  "name": "date-duration",
  "version": "0.2.0",
  "description": "Manipulate Date objects with ISO 8601-formatted durations",
  "main": "dist/duration.js",
  "scripts": {
    "lint": "eslint src test",
    "build": "babel --source-maps -d dist/ src/",
    "test": "npm run lint && npm build && mocha --compilers js:babel-core/register test",
    "clean": "rm -rf dist",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "prepublish": "npm test",
    "postpublish": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smhg/date-duration.git"
  },
  "keywords": [
    "time",
    "duration",
    "iso",
    "iso8601",
    "iso",
    "8601",
    "date"
  ],
  "author": "Sam Hauglustaine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smhg/date-duration/issues"
  },
  "homepage": "https://github.com/smhg/date-duration#readme",
  "devDependencies": {
    "babel-cli": "6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "4.0.5",
    "babel-preset-es2015": "6.1.18",
    "eslint": "1.10.2",
    "mocha": "^2.3.4"
  }
}
