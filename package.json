{
  "name": "date-duration",
  "version": "1.0.0",
  "description": "Manipulate Date objects with ISO 8601-formatted durations",
  "main": "dist/duration.js",
  "scripts": {
    "lint": "eslint src test",
    "build": "babel --source-maps -d dist/ src/",
    "mocha": "TZ=Europe/Brussels mocha --compilers js:babel-register test && TZ=America/New_York mocha --compilers js:babel-register test",
    "test": "npm run lint && npm run build && npm run mocha",
    "clean": "rm -rf dist",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run test",
    "postpublish": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smhg/date-duration.git"
  },
  "keywords": [
    "time",
    "duration",
    "iso",
    "iso8601",
    "iso",
    "8601",
    "date"
  ],
  "author": "Sam Hauglustaine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smhg/date-duration/issues"
  },
  "homepage": "https://github.com/smhg/date-duration#readme",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-register": "6.18.0",
    "eslint": "3.13.1",
    "mocha": "3.2.0"
  }
}
